        <netty-common-config.version>1.0-SNAPSHOT</netty-common-config.version>
        <netty-common-facade.version>1.0-SNAPSHOT</netty-common-facade.version>
        <netty-common-core.version>1.0-SNAPSHOT</netty-common-core.version>
        <netty-common-server.version>1.0-SNAPSHOT</netty-common-server.version>
