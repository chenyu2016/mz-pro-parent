    <groupId>com.cy.netty</groupId>
    <artifactId>mz-pro-parent</artifactId>
    <packaging>pom</packaging>
    <version>1.0-SNAPSHOT</version>
    <modules>
        <module>mz-pro-config</module>
        <module>mz-pro-core</module>
        <module>mz-pro-facade</module>
        <module>mz-pro-server</module>
        <module>mz-pro-client</module>
